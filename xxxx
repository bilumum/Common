    <script>
        $(function () {

            jQuery.fn.toggleAttr = function (attr, value) {
                return this.each(function () {
                    var $this = $(this);
                    $this.attr(attr) ? $this.removeAttr(attr) : $this.attr(attr, value);
                });
            };

            function attchHoverEvents() {
                $('.smart-rating-wrapper').find('.rate-line-circle').each(function (t, item) {

                    $(item).on('click', function () {
                        var selectedValue = $(this).attr('data-rate-type');
                        var row = $(this).parents('.row');
                        var actionRow = $(this).parents('.row').next();

                        row.find('.rate-line-circle').each(function (i, item) {                            
                            $(item).removeAttr('data-active-rate');
                            $(item).removeAttr('data-selected');
                        });
                       
                        actionRow.find('.rate-box').each(function (i, item) {
                            if ($(item).attr('data-rate-type') !== selectedValue) {
                                $(item).css('display', 'none ');                               
                            }                           
                        });

                        var rateBox = actionRow.find('.rate-box[data-rate-type=' + selectedValue + ']');
                        rateBox.toggle(250);

                        $(item).toggleAttr('data-active-rate', true);

                        if ($(item).attr('data-active-rate')) {
                            for (var i = 1; i <= selectedValue; i++) {
                                var el = row.find('.rate-line-circle[data-rate-type=' + i + ']');
                                el.attr('data-selected', selectedValue);
                            }
                        }
                                             
                    });

                    $(item).hover(function () {
                        var selectedValue = $(this).attr('data-rate-type');
                        var row = $(this).parents('.row');
                        for (var i = 1; i <= selectedValue; i++) {
                            var el = row.find('.rate-line-circle[data-rate-type=' + i + ']');
                            el.addClass('hover');
                            row.find('.rate-line-circle[data-rate-type=' + (i - 1) + ']').addClass('active');
                        }

                    }, function () {
                        var selectedValue = $(this).attr('data-rate-type');
                        var row = $(this).parents('.row');                         
                        for (var i = 1; i <= selectedValue; i++) {
                            var el = row.find('.rate-line-circle[data-rate-type=' + i + ']');                           
                            el.removeClass('hover active');                            
                        }
                    });

                });

                $('.smart-rating-wrapper').find('.rate-action-circle').each(function (t, item) {
                    $(item).on('click', function () {
                        var selectedValue = $(this).parents('.rate-box').attr('data-rate-type');
                        var row = $(this).parents('.row').prev();

                        row.find('.rate-line-circle').each(function (i, item) {
                            $(item).removeAttr('data-final-selected');
                            $(item).find('span').text('');
                        });

                        $(this).parents('.rate-box').toggle(250);
                        var el = row.find('.rate-line-circle[data-rate-type=' + selectedValue + ']');
                        el.find('span').text($(this).find('span').text());
                        el.attr('data-final-selected', true);
                    })
                });

            }

            //function toggleSelectedRateLine(selectedEl, selectedValue) {
            //    var row = selectedEl.parents('.row');
            //    for (var i = 1; i <= selectedValue; i++) {
            //        var el = row.find('.rate-line-circle[data-rate-type=' + i + ']');
            //        el.toggle('selected');
            //    }
            //}           

            attchHoverEvents();
        });
    </script>
</body>
</html>
